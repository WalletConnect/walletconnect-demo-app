    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "postinstall": "./node_modules/.bin/rn-nodeify --hack --install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walletconnect/walletconnect-developer-app.git"
  },
